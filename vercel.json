{
    "version": 2,
    "builds": [
        {
            "src": "src/server/server.js",
            "use": "@vercel/node",
            "config": { "includeFiles": ["src/**"] }
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "src/server/server.js"
        }
    ]
}